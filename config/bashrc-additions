# Demo Config Shell Additions
# Add these lines to your ~/.bashrc or ~/.zshrc

# Load demo config environment
if [ -f "/Users/michaelfoster/Desktop/github/red_hat_github/demo-config/config/environment.sh" ]; then
    source "/Users/michaelfoster/Desktop/github/red_hat_github/demo-config/config/environment.sh"
fi

# Load demo config aliases
if [ -f "/Users/michaelfoster/Desktop/github/red_hat_github/demo-config/config/aliases.sh" ]; then
    source "/Users/michaelfoster/Desktop/github/red_hat_github/demo-config/config/aliases.sh"
fi

# Add demo config scripts to PATH
export PATH="/Users/michaelfoster/Desktop/github/red_hat_github/demo-config/scripts:$PATH"
